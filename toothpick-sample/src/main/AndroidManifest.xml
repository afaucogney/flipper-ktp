<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.toothpick">

  <application
      android:allowBackup="false"
      android:fullBackupContent="false"
      android:label="@string/app_name"
      android:name=".BackpackApplication"
      android:theme="@style/Theme.AppCompat.Light"
      tools:ignore="GoogleAppIndexingWarning,MissingApplicationIcon,UnusedAttribute">

    <activity android:name=".activity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name=".activity.SimpleBackpackItemsActivity" />
    <activity android:name=".activity.AdvancedBackpackItemsActivity" />
    <activity android:name=".activity.AddNewActivity" />

  </application>
</manifest>
