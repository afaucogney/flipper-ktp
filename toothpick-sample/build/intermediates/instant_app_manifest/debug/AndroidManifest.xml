<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kotlin"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="26" />

    <application
        android:name="com.example.toothpick.BackpackApplication"
        android:allowBackup="false"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:fullBackupContent="false"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity android:name="com.example.toothpick.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.toothpick.activity.SimpleBackpackItemsActivity" />
        <activity android:name="com.example.toothpick.activity.AdvancedBackpackItemsActivity" />
        <activity android:name="com.example.toothpick.activity.AddNewActivity" />

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.example.kotlin.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>